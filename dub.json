        {
            "name": "lda-hoffman-sparse",
            "targetName": "lda-hoffman-sparse",
            "targetType": "executable",
            "targetPath": "build",
            "sourceFiles": ["examples/lda_hoffman_sparse.d"]
        }
    ],
    "dependencies": {
        "cpuid": "~>0.2.4"
    }
